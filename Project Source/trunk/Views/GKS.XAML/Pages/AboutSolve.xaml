<Window x:Class="GKS.XAML.Pages.AboutSolve"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TestWindow" Height="322" Width="445">
    <Grid>
        <DataGrid AutoGenerateColumns="False" Height="200" HorizontalAlignment="Left" Margin="45,12,0,0" Name="dataGridEmployees" VerticalAlignment="Top" Width="337" >
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Path=EmployeeID}" Header="ID" />
                <DataGridTextColumn Binding="{Binding Path=EmployeeName}" Header="Name" />
                <DataGridTextColumn Binding="{Binding Path=EmployeeDesignation}" Header="Designation" />
            </DataGrid.Columns>
        
        </DataGrid>
        <Button Content="Export" Height="23" HorizontalAlignment="Left" Margin="307,234,0,0" Name="buttonExport" VerticalAlignment="Top" Width="75" Click="buttonExport_Click" />
    </Grid>
</Window>
